{
  "name": "stylelint-config-wildbit",
  "version": "1.0.0",
  "description": "Shareable stylelint config",
  "main": "index.js",
  "bin": {
    "precommit-stylelint": "./precommit-stylelint.sh"
  },
  "scripts": {
    "tape": "babel-tape-runner \"__tests__/**/*.js\"",
    "test": "npm run tape"
  },
  "pre-push": ["test"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wildbit/stylelint-config-wildbit.git"
  },
  "keywords": [
    "stylelint",
    "config"
  ],
  "author": "Andrey Okonetchnikov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wildbit/stylelint-config-wildbit/issues"
  },
  "homepage": "https://github.com/wildbit/stylelint-config-wildbit#readme",
  "dependencies": {
    "stylelint-selector-bem-pattern": "^0.2.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-tape-runner": "^2.0.0",
    "glob": "^6.0.3",
    "pre-push": "^0.1.1",
    "stylelint": "^6.0.3",
    "tape": "^4.4.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
